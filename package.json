{
  "name": "jest-generator",
  "displayName": "JestGen",
  "publisher": "TWang",
  "icon": "./public/JestGen.png",
  "repository": {"type": "git", "url": "https://github.com/LonesomeTown/JestGen"},
  "keywords": [
    "jest",
    "generator",
    "typescript",
    "javascript"
  ],
  "description": "Automatically generate Jest for Typescript project",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createTestFile",
        "title": "Generate Jest File"
      }
    ],
    "keybindings": [
      {
        "command": "extension.createTestFile",
        "key": "cmd+alt+t",
        "when": "(editorTextFocus || editorHasSelection) && (editorLangId == 'typescript'|| editorLangId == 'javascript')"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.createTestFile",
          "group": "navigation",
          "when": "editorHasSelection && (editorLangId == 'typescript'|| editorLangId == 'javascript')"
        }
      ]
    },
    "jsonValidation": [
      {
        "fileMatch": ".jestgen.json",
        "url": "https://lonesometown.github.io/JestGen/json-schema.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/supertest": "^2.0.12",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "jest": "^29.5.0",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@babel/parser": "^7.22.5",
    "@babel/types": "^7.22.5"
  }
}
